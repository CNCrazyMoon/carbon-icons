{
  "name": "@console/bluemix-icons",
  "version": "2.1.0",
  "description": "SVG icons as sprite for Bluemix",
  "main": "sprite.svg",
  "private": false,
  "scripts": {
    "dev": "nodemon -e html,svg server.js",
    "build:sprite": "gulp build && gulp copy",
    "build:bluemix-icons": "gulp buildIconsSvg && gulp copyToRoot",
    "prebuild": "gulp clean",
    "build": "npm run build:sprite && npm run build:bluemix-icons && gulp xml2json"
  },
  "engines": {
    "node": "~4.0.0",
    "npm": "~2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:Bluemix/bluemix-icons.git"
  },
  "keywords": [
    "icons",
    "bluemix",
    "ibm"
  ],
  "maintainers": [
    {
      "name": "Brian Han",
      "email": "bthan@us.ibm.com"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "del": "2.2.0",
    "gh-pages": "0.11.0",
    "gulp": "3.9.0",
    "gulp-gh-pages": "0.5.4",
    "gulp-rename": "1.2.2",
    "gulp-svg-sprite": "1.2.18",
    "gulp-svgo": "1.0.3",
    "nodemon": "1.9.2",
    "remove-svg-properties": "0.3.2",
    "express": "4.13.4",
    "svgxuse": "1.1.16"
  }
}
