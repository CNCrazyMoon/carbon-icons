{
  "name": "@console/bluemix-icons",
  "version": "4.0.2",
  "description": "SVG icons as sprite for Bluemix",
  "main": "bluemix-icons.js",
  "private": false,
  "scripts": {
    "dev": "nodemon -e html,svg server.js",
    "prebuild": "gulp clean",
    "build:bluemix-icons": "gulp build:svg && gulp copy && gulp build:json",
    "build:legacy": "gulp build:svg --legacy && gulp copy --legacy && gulp build:json --legacy",
    "build": "npm run build:bluemix-icons && npm run build:legacy",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:Bluemix/bluemix-icons.git"
  },
  "engines": {
    "node": "~6.0.0",
    "npm": "~3.0.0"
  },
  "keywords": [
    "icons",
    "bluemix",
    "ibm"
  ],
  "files": [
    "scripts",
    "legacy-icons.js",
    "bluemix-icons.js",
    "bluemix-icons.json",
    "bluemix-icons.svg",
    "sprite.svg",
    "icons.json"
  ],
  "maintainers": [
    {
      "name": "Brian Han",
      "email": "bthan@us.ibm.com"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "chalk": "1.1.3",
    "del": "2.2.0",
    "express": "4.13.4",
    "gh-pages": "0.11.0",
    "gulp": "3.9.0",
    "gulp-dom": "0.9.0",
    "gulp-gh-pages": "0.5.4",
    "gulp-rename": "1.2.2",
    "gulp-svg-sprite": "1.2.18",
    "gulp-svgo": "1.0.3",
    "gulp-util": "3.0.7",
    "jest": "16.0.2",
    "nodemon": "1.9.2",
    "remove-svg-properties": "0.3.2",
    "svgxuse": "1.1.16",
    "xml2js": "0.4.17"
  }
}
